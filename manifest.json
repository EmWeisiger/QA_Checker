{
	"manifest_version": 2,
	"name": "CheckIt",
	"version": "1.0",
	"description": "Client services approval tool.",
	"icons":
    {
        "16": "images/icon.png",
        "48": "images/icon.png"
     },
    "web_accessible_resources":
    [
        "images/*.png",
        "valueGrabber.html"
    ],
	"content_scripts":
	[
		{
			"matches": ["*://WEBSITE/*","*://WEBSITE/*"],
			"js": ["scripts/jquery-2.1.3.min.js", "scripts/main.js"],
			"run_at": "document_end"
		}
	]
}
